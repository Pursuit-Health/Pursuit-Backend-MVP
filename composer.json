{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2",
        "league/fractal": "^0.16.0",
        "spatie/laravel-fractal": "^4.01",
        "illuminate/mail": "^5.4",
        "guzzlehttp/guzzle": "6.3.0",
        "tymon/jwt-auth": "1.0.0-beta.3",
        "league/flysystem": "1.0.41",
        "league/flysystem-aws-s3-v3": "1.0.18"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9",
        "barryvdh/laravel-ide-helper": "v2.4.0",
        "doctrine/dbal": "~2.3",
        "laravel/tinker": "v1.0.2",
        "laravelista/lumen-vendor-publish": "2.0.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "gen-meta": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models -W"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true
}
